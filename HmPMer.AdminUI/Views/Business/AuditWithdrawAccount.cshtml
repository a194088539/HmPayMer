

@{
    ViewBag.Title = "商户管理";
    Layout = "~/Views/Shared/_Content.cshtml";
}

@section HeadSection{
    <script src="/scripts/business/auditwithdrawaccount.js?@ViewBag.StaticVersion"></script>

}

<div class="layui-form">

    <blockquote class="layui-elem-quote quoteBox">
        <form class="layui-form" id="form_seartch">
            <div class="layui-inline">
                @Html.Hidden("AccountType", Request["AccountType"])
                <div class="layui-input-inline">
                    @(Request["AccountType"]=="1"? "商户号" : "代理Id") ：
                </div>
                <div class="layui-input-inline">
                    <input type="text" class="layui-input" id="UserId" name="UserId"  />
                </div>
                
                <div class="layui-input-inline">
                    审核状态：
                </div>
                <div class="layui-input-inline">
                    <select id="WithdrawStatus" name="WithdrawStatus" lay-verify="" lay-filter="WithdrawStatus">
                        <option value="-1">请选择</option>
                        <option value="3" selected>待审核</option>
                        <option value="1">通过</option>
                        <option value="2">不通过</option>
                    </select>
                </div>
                <a class="layui-btn search_btn" data-type="reload">搜索</a>
            </div>

        </form>
    </blockquote>
    <table id="AuditWithdrawAccountList" lay-filter="AuditWithdrawAccountList"></table>
    <!--操作-->
    <script type="text/html" id="AuditWithdrawAccountListBar">
        {{#  if(d.WithdrawStatus == "3"){ }}
        <a class="layui-btn-xs layui-btn-mycss" lay-event="btn_audit">审核</a>
        {{#  }}}
    </script>
</div>

