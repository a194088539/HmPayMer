
@{
    ViewBag.Title = "入账方案";
    Layout = "~/Views/Shared/_Content.cshtml";
}

@section HeadSection{
    <script src="/scripts/withdraw/accountscheme.js?@ViewBag.StaticVersion"></script>
}
<div class="layui-form">
    <blockquote class="layui-elem-quote quoteBox">
        <form class="layui-form" id="form_seartch">
            <div class="layui-inline">
                <div class="layui-input-inline">
                    <input type="text" class="layui-input searchVal" id="Name" name="Name" placeholder="请输入搜索名称" />
                </div>
                <a class="layui-btn search_btn" data-type="reload">搜索</a>
            </div>
            <div class="layui-inline">
                <a class="layui-btn layui-btn-normal addAdmin_btn" id="addadmin_btn">
                    <i class="layui-icon">&#xe608;</i>添加
                </a>
            </div>
        </form>
    </blockquote>
    <table id="AccountSchemeList" lay-filter="AccountSchemeList"></table>

    <!--操作-->
    <script type="text/html" id="AccountSchemeListBar">
        <a class="layui-btn layui-btn-xs" lay-event="edit">编辑</a>
        <a class="layui-btn layui-btn-xs" lay-event="setdeail">设置</a>
        <a class="layui-btn layui-btn-xs layui-btn-danger" lay-event="deldata">删除</a>
    </script>
</div>

