
@{
    ViewBag.Title = "入账处理";
    Layout = "~/Views/Shared/_Content.cshtml";
}

@section HeadSection{
    <script src="/Scripts/jquery-1.8.3.min.js?@ViewBag.StaticVersion"></script>
    <script src="/scripts/Withdraw/orderaccounttarnindex.js?@ViewBag.StaticVersion"></script>
    <script src="/Scripts/My97DatePicker/WdatePicker.js?@ViewBag.StaticVersion"></script>
}

<div class="layui-form">
    <blockquote class="layui-elem-quote quoteBox">
        <form class="layui-form" id="form_seartch">
            <div class="layui-inline">
                <div class="layui-input-inline">
                    商户ID：
                </div>
                <div class="layui-input-inline" style="width:100px">
                    <input type="text" class="layui-input" id="UserId" name="UserId" placeholder="商户ID" />
                </div>
                <a class="layui-btn search_btn" data-type="reload">搜索</a>
            </div>
        </form>
    </blockquote>
    <table id="OrderAccountTarnIndexList" lay-filter="OrderAccountTarnIndexList"></table>
</div>

<!--操作-->
<script type="text/html" id="BusinessListBar">
    <a class="layui-btn-xs layui-btn-mycss" lay-event="btn_confirm">确认入账</a>
</script>